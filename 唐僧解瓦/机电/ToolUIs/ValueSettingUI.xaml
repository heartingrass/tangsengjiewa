<Window x:Class="唐僧解瓦.机电.ToolUIs.ValueSettingUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:唐僧解瓦.机电.ToolUIs"
             mc:Ignorable="d" Height="119.291" Width="233.071">
    <Grid>
        <Border BorderThickness="2" CornerRadius="5" BorderBrush="BlueViolet">

            <StackPanel Orientation="Vertical">
                <TextBlock Name="valuesettinglabel" Text="支管提升高度值" Margin="5" Width="90" HorizontalAlignment="Left"></TextBlock>
                <TextBox Name="settingValue" Text="200" Width="90" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"></TextBox>
                <Button Name="Okbtn" Content="OK" Width="60" HorizontalAlignment="Right" Margin="0,0,20,0" Click="Okbtn_Click"></Button>
            </StackPanel>
        </Border>
    </Grid>
</Window>
